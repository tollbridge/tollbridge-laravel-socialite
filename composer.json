{
    "name": "tollbridge/laravel-socialite",
    "description": "Tollbridge.co service provider for Laravel Socialite authentication.",
    "keywords": [
        "tollbridge",
        "tollbridge.co",
        "tollbridge laravel",
        "tollbridge-socialite-provider"
    ],
    "homepage": "https://github.com/tollbridge/tollbridge-laravel-socialite",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeff Ochoa",
            "email": "jeff@square1.io",
            "homepage": "https://www.tollbridge.co",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "laravel/socialite": "^5.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.18",
        "orchestra/testbench": "^6.11",
        "phpunit/phpunit": "^9.3",
        "psalm/plugin-laravel": "^1.4",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Tollbridge\\Socialite\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tollbridge\\Socialite\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tollbridge\\Socialite\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
